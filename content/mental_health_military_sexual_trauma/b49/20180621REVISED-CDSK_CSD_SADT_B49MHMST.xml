               <row>
                  <entry align="right" colsep="0" rowsep="0" colname="c2">item:</entry>
                  <entry rowsep="0" namest="c3" nameend="c7">Declines to answer</entry>
               </row>
               <row xmlns:a="urn:hl7-org:cql-annotations:r1"
