                  <entry rowsep="0" namest="c2" nameend="c7">EnumerationConstraint (List) </entry>
               </row>
               <row>
                  <entry align="right" colsep="0" rowsep="0" colname="c1">item:</entry>
                  <entry rowsep="0" namest="c2" nameend="c7">No</entry>
               </row>
               <row>
                  <entry align="right" colsep="0" rowsep="0" colname="c1">item:</entry>
                  <entry rowsep="0" namest="c2" nameend="c7">Yes</entry>
